<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hanains.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:largeHeap="true">

        <activity
            android:name=".LocationDemoActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <activity android:name=".demos.MapViewDemoActivity" android:label="@string/map_view_demo_title"/>

    <activity android:name=".demos.MarkerDemoActivity" android:label="@string/marker_demo_title"/>

    <activity android:name=".demos.PolygonDemoActivity" android:label="@string/polygon_demo_title"/>

    <activity android:name=".demos.LocationDemoActivity" android:label="@string/location_demo_title"/>

    <activity android:name=".demos.CameraDemoActivity" android:label="@string/camera_demo_title"/>

    <activity android:name=".demos.EventsDemoActivity" android:label="@string/events_demo_title"/>

    <activity android:name=".demos.SearchDemoActivity" android:label="@string/search_demo_title"/>

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
</manifest>
